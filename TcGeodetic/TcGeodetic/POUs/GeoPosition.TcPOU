      <Declaration><![CDATA[METHOD PUBLIC Clear
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[Latitude_ := 0;
Longitude_ := 0;
Elevation_ := 0;]]></ST>
      </Implementation>
    </Method>
