    <Method Name="IsEqual" Id="{f18eb5d7-5a74-4649-8b7c-157f6655d827}" FolderPath="Comparators\">
      <Declaration><![CDATA[METHOD PUBLIC IsEqual : BOOL
VAR_INPUT
	P : REFERENCE TO GeoPosition;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IsEqual := IsNearlyEqual(P,0.0);]]></ST>
      </Implementation>
    </Method>
    <Method Name="IsNearlyEqual" Id="{aa31bf7f-c28c-42b9-bc7f-9674c88e4763}" FolderPath="Comparators\">
      <Declaration><![CDATA[METHOD PUBLIC IsNearlyEqual : BOOL
VAR_INPUT
	P : REFERENCE TO GeoPosition;
	Precision : LREAL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IsNearlyEqual := ABS(P.LatitudeRad 		- LatitudeRad) 	<= Precision
             AND ABS(P.LongitudeRad 	- LongitudeRad) <= Precision
             AND ABS(P.Elevation 		- Elevation)	<= Precision;]]></ST>
      </Implementation>
    </Method>
