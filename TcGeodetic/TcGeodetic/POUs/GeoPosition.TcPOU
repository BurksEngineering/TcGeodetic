    <Method Name="SetFromCartesianPosition" Id="{048ddb8f-7491-418a-a8bf-7b2c0d60621f}" FolderPath="Setters\">
      <Declaration><![CDATA[METHOD PUBLIC SetFromCartesianPosition
VAR_INPUT
	V : Vector3;
	Ref : REFERENCE TO GeoEllipsoid;
END_VAR
